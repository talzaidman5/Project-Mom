<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AllTheEventsOfClient"
    android:background="@drawable/pic3">
    <TextView
        android:id="@+id/waitingToConfirmation_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="10dp"
        android:textSize="23dp"
        android:textColor="#000000"
        android:text="The queues that waiting for your approval:"></TextView>
    <ListView
        android:layout_width="wrap_content"
        android:layout_height="300dp"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/waitingToConfirmation_title"
        android:id="@+id/waitingToConfirmation_ListView">
    </ListView>


    <Button
        android:id="@+id/allEventsOfClient_back"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="25dp"
        android:text="back"
        android:layout_alignParentLeft="true"
        android:layout_margin="10dp"
        android:layout_alignParentBottom="true"
        android:background="@color/buttonColor"></Button>

</RelativeLayout>
